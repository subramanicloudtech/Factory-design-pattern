public class Member
{
  public void Register()
  {
    
  }
  
  public void Notify()
  {
    
  }
}

public class AnnualMember : Member
{
  
}

public class LifetimeMember : Member
{
  
}

public class TemporaryMember : Member
{
  
}

Enum MemberType
{
  Annual,
  Lifetime,
  Temporary
}

public class MembershipManager
{
    Member m;
    MemberType memberType = MemberType.Annual;
    m = MemberFactory.CreateMember(memberType);
    
    m.Register();
    m.Notify();
}

public class MemberFactory
{
    public static Member CreateMember(MemberType type)
    {
        if(type == 'Annual')
        {
           m = new AnnualMember();
        }
    
        if(type == 'Lifetime')
        {
            m = new LifetimeMember();
        }

        else
        {
            m = new LifetimeMember();
        }
        
        return m;
    }
}

public class Client
{
    public void ProcessMember
    {
        MemberType type;
        MembershipManager m1 = new MembershipManager();
        m1.
    }
}
